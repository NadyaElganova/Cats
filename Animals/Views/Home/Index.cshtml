@model IEnumerable<Cat>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    @if (ViewBag.error != null)
    {
        <h1 class="text-danger">@ViewBag.error</h1>
    }
    else if (Model.Count() > 0)
    {
        <h1 class="display-4">Select breed</h1>
         
        <select id="select-option" class="form-select" aria-label="Пример выбора по умолчанию">
            <option selected>Select breed</option>
            @foreach (Cat item in @Model)
            {
                <option data-cat="@item">@item.name</option>
            }
        </select>   
        
    }
</div>






